fun main()
    qubit q1;
    qubit q2;

    @ Initial superposition
    H(q1);
    H(q2);

    @ First measurement
    var r1 = measure q1;
    var r2 = measure q2;
    print("Initial measurement:");
    print("q1: ", r1);
    print("q2: ", r2);

    @ Move forward in time by 2 steps (simulation snapshot)
    fastforward 2;

    @ Apply more gates
    H(q1);
    CNOT(q1, q2);

    r1 = measure q1;
    r2 = measure q2;
    print("After H + CNOT:");
    print("q1: ", r1);
    print("q2: ", r2);

    @ Rewind 1 step (go back to previous snapshot)
    rewind 1;

    r1 = measure q1;
    r2 = measure q2;
    print("After rewind:");
    print("q1: ", r1);
    print("q2: ", r2);

    return 0;
nuf
