fun main() 
    var v1 = vector(3, [1, 2, 3]);
    var v2 = vector(3, [4, 5, 6]);

    print("Addition");
    var v3 = v1 + v2;
    print("v3 = ", v3);  

    var dp = dot(v1, v2);
    print("dot = ", dp);

    print("Subtraction");
    var v4 = v1 - v2;
    print("v4 = ", v4);

    var c = cross(v1, v2);
    print("cross = ", c);

    var mag = abs(v1);
    print("mag = ", mag); 
    return 0;
