fun main() 
    var z1 = complex(3, 4);
    var z2 = complex(1, 2);
    var z3 = z1 + z2;
    print("addition:");
    print("z3.real = ", z3.real);  
    print("z3.imag = ", z3.imag);

    var z4 = z1 - z2;
    print("\nsubtraction:");
    print("z4.real = ", z4.real);  
    print("z4.imag = ", z4.imag); 

    var z5= z1 * z2;
    print("\nmultiplication");
    print("z5.real = ", z5.real);  
    print("z5.imag = ", z5.imag);   

    var z6 = z1 / z2;
    print("\ndivision");
    print("z6.real = ", z6.real);  
    print("z6.imag = ", z6.imag);  

    print("\nabs");
    var m = abs(z1);
    print("abs(z1) = ", m);    
    return 0;   
