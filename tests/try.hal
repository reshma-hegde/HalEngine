fun safe_divide(var a, var b)
    if b == 0
        raise "Cannot divide by zero!";
    fi
    return a / b;
nuf

fun main()
    print("Enter numerator: ");
    var num = input() : float;

    print("Enter denominator: ");
    var den = input() : float;

    var result = 0.0;

    branch
        result = safe_divide(num, den);
        print("Division successful! Result: ", result);
    handle e
        print("Error: ", e);
        result = 0.0;
    end

    print("consumes");
    var int_result = result : int;
    print("Result cast to int: ", int_result);

    
    var str_result = result : str;
    print("Result as string: ", str_result);

    return 0;
nuf
